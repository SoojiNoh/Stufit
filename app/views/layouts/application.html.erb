    <!-- Favicons-->
    <link rel="icon" href="assets/favicon/favicon-32x32.png" sizes="32x32">
    <!-- Favicons-->
    <link rel="apple-touch-icon-precomposed" href="assets/favicon/apple-touch-icon-152x152.png">
    <!-- For iPhone -->
    <meta name="msapplication-TileColor" content="#00bcd4">
    <meta name="msapplication-TileImage" content="assets/favicon/mstile-144x144.png">
    <!-- For Windows Phone -->
    <!-- CORE CSS-->

    <link href="<%= asset_path "materialize.css" %>" type="text/css" rel="stylesheet" media="screen,projection">
    <link href="<%= asset_path "style.css" %>"type="text/css" rel="stylesheet" media="screen,projection">


