<p id="notice"><%= notice %></p>

<h1>전공 스토리</h1>

<table>
  <thead>
    <tr>
      <th>일정</th>
      <th>설명</th>
      <th>장소</th>
      <th>첨부파일</th>
      <th>분류</th>
      <th>대학</th>
      <th colspan="3"></th>
    </tr>
  </thead>


    <tbody>
      <% @major_stories.each do |major_story| %>
        <tr>
          <td><%= major_story.title %></td>
          <td><%= major_story.content %></td>
          <td><%= major_story.place %></td>
          <td><%= major_story.file %></td>
          <td><%= major_story.major_type %></td>
          <td><%= major_story.university_id %></td>
          <td><%= link_to '자세히', major_story %></td>
          <td><%= link_to '수정', edit_major_story_path(major_story) %></td>
          <td><%= link_to '삭제', major_story, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
</table>

<br>

<%= link_to '전공추가', new_major_story_path %>

