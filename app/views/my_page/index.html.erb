      <li class="tab col s3"><a href="#test2">모의고사</a></li>
    </ul>
  </div>
  <div id="test1" class="col s12">
    <%= link_to "활동", activities_path %>
  </div>
  <div id="test2" class="col s12">
    <%= render partial: 'mock_tests/index', locals: { :mock_tests => @mock_tests } %>
